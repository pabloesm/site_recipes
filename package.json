{
  "name": "site_recipes",
  "version": "1.0.0",
  "description": "backbone.js example",
  "main": "app.js",
  "scripts": {
    "initdb": "mongod -dbpath ~/work/mongodb/data/db",
    "initdb_pi": "mongod --journal -dbpath ~/pablo/mongodb/data/db/",
    "start": "node server.js",
    "build-app": "browserify site/js/app/app.js -o site/app.bundle.js -dv -t [ babelify --presets [ es2015 ] ]",
    "build-admin": "browserify site/js/app/admin.js -o site/admin.bundle.js -dv -t [ babelify --presets [ es2015 ] ]",
    "watch-app": "watchify site/js/*/*.js -o site/app.bundle.js -dv -t [ babelify --presets [ es2015 ] ]"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pabloesm/site_recipes.git"
  },
  "author": "Pablo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pabloesm/site_recipes/issues"
  },
  "homepage": "https://github.com/pabloesm/site_recipes",
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "backbone": "^1.3.3",
    "body-parser": "^1.13.3",
    "express": "^5.0.0-alpha.2",
    "handlebars": "^4.0.5",
    "jquery": "^2.1.4",
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.2.7",
    "multer": "^1.0.3",
    "path": "^0.12.7",
    "slick-carousel": "^1.5.8",
    "slug": "^0.9.1",
    "to-markdown": "^1.3.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "rimraf": "^2.5.1",
    "watchify": "^3.7.0"
  }
}
